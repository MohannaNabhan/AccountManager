{
  "manifest_version": 3,
  "name": "Account Manager Extension (Simple)",
  "version": "1.0.0",
  "description": "Versi√≥n simplificada para debugging",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background-simple.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Account Manager",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}